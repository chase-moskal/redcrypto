{
	"name": "redcrypto",
	"version": "0.0.0-dev.0",
	"description": "crypto that works places",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com",
	"type": "module",
	"main": "",
	"module": "",
	"files": [
		"dist",
		"source",
		"dist-cjs"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile-esm compile-cjs generate-keys && run-s test",
		"clean": "rimraf dist dist-cjs && mkdirp dist dist-cjs",
		"compile-esm": "tsc",
		"compile-cjs": "tsc --module commonjs --outDir dist-cjs",
		"watch": "run-p watch-esm watch-cjs",
		"watch-esm": "npm run compile-esm -s -- --watch",
		"watch-cjs": "npm run compile-esm -s -- --watch",
		"generate-keys": "./generate-keys",
		"start": "serve",
		"test": "run-p test-node test-puppeteer",
		"test-node": "cynic node --label='redcrypto tests (node)' dist/redcrypto.test.js",
		"test-puppeteer": "cynic puppeteer --label='redcrypto tests (browser)' dist/redcrypto.test.js"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/renraku.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/renraku/issues"
	},
	"homepage": "https://github.com/chase-moskal/renraku#readme",
	"devDependencies": {
		"@types/jest": "^24.9.0",
		"@types/node": "^13.1.7",
		"cynic": "file:../cynic/cynic-0.1.0-dev.1.tgz",
		"importly": "0.1.0-dev.0",
		"jest": "^24.9.0",
		"jest-puppeteer": "^4.4.0",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^2.0.0",
		"replace": "^1.1.5",
		"rimraf": "^3.0.0",
		"serve": "^11.3.0",
		"typescript": "^3.7.4"
	},
	"dependencies": {}
}
