		"javascript",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/redcrypto.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/redcrypto/issues"
	},
	"homepage": "https://github.com/chase-moskal/redcrypto#readme",
	"devDependencies": {
		"@types/node": "^12.12.31",
		"cynic": "0.1.0-dev.8",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^1.0.3",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^2.1.1",
		"replace": "^1.1.5",
