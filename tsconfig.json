{
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": "s",
		"outDir": "x",
		"target": "esnext",
		"lib": ["es6", "dom"],
		"module": "esnext",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"declaration": true,
		"skipLibCheck": true
	},
	"include": [
		"s/**/*"
	]
}
